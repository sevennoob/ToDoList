<%- include("header") -%>

  <div class="box" id="heading">
    <h1><%= title %></h1>
  </div>

  <div class="box">
    <% for(let i of items){ %>
      <div class="item">
        <input type="checkbox">
        <p><%= i%></p>
      </div>
    <% } %>

    <form class="" action="/" method="post">
      <input type="text" name="new_item" placeholder="New Item" autocomplete="off">
      <button type="submit" name="button" value=<%= title %>>+</button>
    </form>

  </div>

  <%- include("footer") -%>
